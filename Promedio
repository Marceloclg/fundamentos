#include <iostream>

using namespace std;

int main()
{
    float numero1, numero2, promedio;
    cout<<"Ingresar numero 1: ";
    cin>>numero1;
    cout<<"Ingresar numero 2: ";
    cin>>numero2;
    promedio= numero1+numero2;
    cout<<"Ingresar numero 3: ";
    cin>>numero1;
    promedio= promedio+ numero1;
    cout<<"Ingresar numero 4: ";
    cin>>numero1;
    promedio= promedio+numero1;
    cout<<"Ingresar numero 5: ";
    cin>>numero1;
    promedio= promedio+numero1;
    cout<<"El promedio es: "<<promedio/5;

    return 0;
}
